        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="FirstName" HorizontalAlignment="Left" Margin="284,86,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="392" Height="26"/>
