        <TextBox x:Name="GroupName" HorizontalAlignment="Left" Margin="284,146,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="392" Height="24" RenderTransformOrigin="0.5,0.5"/>
        <Button Content="Save" HorizontalAlignment="Left" Margin="284,177,0,0" VerticalAlignment="Top" Width="103" Click="Button_Click_1"/>
        <Label Content="FirstName" HorizontalAlignment="Left" Margin="209,86,0,0" VerticalAlignment="Top"/>
